  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-virtuoso": "^4.14.0"
  },
  "scripts": {
    "start": "cross-env PORT=3000 react-scripts start",
